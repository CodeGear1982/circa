
-- Infix expressions
print(to-source(1 + 2))
print(to-source(3 * 4 - 2))

-- Arrow expressions
print(to-source(4 -> sqrt))

-- Normal functions
print(to-source(add(1,2)))
print(to-source(average(4,3,2,1)))

-- Nested expressions
print(to-source(add(3*4, 4/2)))

-- Named inputs
a = 1
b = 3 + 4
print(to-source(a + b))
print(to-source(add(a,b)))

-- Name binding
my-name = to-source(1 + 2)
print(to-source(my-name))
