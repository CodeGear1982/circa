
my_map = map(int,int)

my_map(5) := 3
print('should be 3: ', my_map(5))

my_map(3) := 1
print('should be 1: ', my_map(3))

string_map = map(string,int)
string_map('key1') := 5
print('should be 5: ', string_map('key1'))

int_map = map(int,string)
int_map(18) := 'apple'
print('should be apple: ', int_map(18))

