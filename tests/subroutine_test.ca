
function square(a) {
    return a*a
}

function dist(a,b) {
    return sqrt(square(a) + square(b))
}

function three_a_plus_two_b(a,b) {
  return 3 * a + 2 * b
}

assert(square(2) == 4)
assert(square(-1) == 1)

x = 4 + 5
assert(square(x) == 81)

x = 1
y = 1

result1 = three_a_plus_two_b(x,y)

assert(result1 == 5)


