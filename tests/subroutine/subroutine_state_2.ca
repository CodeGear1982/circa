
-- Call a function with arguments and a state arg

def f(int i, int j) -> int
  state s = j
  s += i
  return s
  
int = make_stack(f)
result = int.call(1, 5)
print('should be 6: ' result)
result = int.call(3, 0)
print('should be 9: ' result)
result = int.call(-4, 0)
print('should be 5: ' result)
result = int.call(1, 0)
print('should be 6: ' result)
