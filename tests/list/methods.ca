
-- Cycle
assert([0 1 2].cycle(1) == 1)
assert([0 1 2].cycle(3) == 0)
assert([0 1 2].cycle(4) == 1)
assert([0 1 2].cycle(6) == 0)
assert([0 1 2].cycle(-1) == 2)
assert([0 1 2].cycle(-5) == 1)
-- Join
assert([].join("x") == "")
assert([1].join("x") == "1")
assert([1, 2].join("x") == "1x2")
assert([1 2 3].join("-") == "1-2-3")
assert([1 2 3].join("") == "123")
assert([1 2 3].join(", ") == "1, 2, 3")

-- Slice
assert([].slice(0,0) == [])
assert([].slice(0,-1) == [])
assert([].slice(-1,0) == [])
assert([].slice(-1,5) == [])
assert([0].slice(0,1) == [0])
assert([0].slice(0,0) == [])
assert([1 2 3].slice(0,-1) == [1 2])
assert([1 2 3].slice(0,2) == [1 2])
assert([1 2 3].slice(0,3) == [1 2 3])
assert([1 2 3].slice(2,3) == [3])
assert([1 2 3].slice(2,0) == [])

-- Get
assert([1 2 3].get(0) == 1)

-- Empty
assert([].empty)
assert(not [0].empty)

-- Last
assert([0].last == 0)
assert([0 1 2].last == 2)

-- Resize
a = [1 2]
@a.resize(4)
assert(a == [1 2 nil nil])
@a.resize(2)
assert(a == [1 2])
@a.resize(1)
assert(a == [1])
@a.resize(0)
assert(a == [])

-- Append
a = [1 2]
@a.append(3)
assert(a == [1 2 3])
