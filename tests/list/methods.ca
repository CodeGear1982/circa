
-- Join
assert([].join("x") == "")
assert([1].join("x") == "1")
assert([1, 2].join("x") == "1x2")
assert([1 2 3].join("-") == "1-2-3")
assert([1 2 3].join("") == "123")
assert([1 2 3].join(", ") == "1, 2, 3")

-- Slice
assert([].slice(0,0) == [])
assert([].slice(0,-1) == [])
assert([].slice(-1,0) == [])
assert([].slice(-1,5) == [])
assert([0].slice(0,1) == [0])
assert([0].slice(0,0) == [])
assert([1 2 3].slice(0,-1) == [1 2])
assert([1 2 3].slice(0,2) == [1 2])
assert([1 2 3].slice(0,3) == [1 2 3])
assert([1 2 3].slice(2,3) == [3])
assert([1 2 3].slice(2,0) == [])

-- Get
assert([1 2 3].get(0) == 1)

-- Empty
assert([].empty)
assert(not [0].empty)

-- Last
assert([0].last == 0)
assert([0 1 2].last == 2)

-- Pop
a = [1 2 3]
assert(a.pop == 3)
assert(a == [1 2])
assert(a.pop == 2)
assert(a == [1])
assert(a.pop == 1)
assert(a == [])
