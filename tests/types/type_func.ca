assert(type(1) == int)
assert(type(1.0) == number)
assert(type('s') == String)

def runtime_typing(int i) -> any
    if i == 0
        return 1
    elif i == 1
        return 1.0
    else
        return "hi"

assert(type(runtime_typing(0)) == int)
assert(type(runtime_typing(1)) == number)
assert(type(runtime_typing(2)) == String)
