

type T {
    int i
}

def T.f(self, int i)
    state s = 0
    s += i
    self.i = i
    print('called T.f with: ' self ', ' s ', ' i)

    if i == 0
        print('doing recursive call..')
        s2 = null
        self.f(5, state = s2)
        state nested = s2.extract_state
        print('finished recursive call, nested = ' nested)


t = make(T)
t.i = 2

s = null
t.f(0, state = s)
print('s = ' s.extract_state)
t.f(1, state = s)
print('s = ' s.extract_state)
