
type T {
    int i
}

def T.f(self, int i)
    state s = 0
    s += i
    self.i = i
    print('called T.f with: ' self ', ' s ', ' i)


t = make(T)
t.i = 2

s = null
t.f(1, state = s)
print('s = ' s.extract_state)
t.f(2, state = s)
print('s = ' s.extract_state)
t.f(3, state = s)
print('s = ' s.extract_state)
