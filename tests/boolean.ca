
-- Check 'true' and 'false' constants
print(concat('should be true: ', true.to_string))
print(concat('should be false: ', false.to_string))

-- and
print(concat('and(true,true): ', and(true,true).to_string))
print(concat('and(true,false): ', and(true,false).to_string))
print(concat('and(false,true): ', and(false,true).to_string))
print(concat('and(false,false): ', and(false,false).to_string))

-- or
print(concat('or(true,true): ', or(true,true).to_string))
print(concat('or(true,false): ', or(true,false).to_string))
print(concat('or(false,true): ', or(false,true).to_string))
print(concat('or(false,false): ', or(false,false).to_string))

