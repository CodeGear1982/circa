-- operator precedence
assert(1 + 2 * 3 == 7)
assert(1 * 2 + 3 == 5)
assert(5 - 4 + 3 == 4)
assert(1 - 2 * 3 == -5)
assert(1 + 4 / 2 == 3)
assert(1 - 4 / 2 == -1)
