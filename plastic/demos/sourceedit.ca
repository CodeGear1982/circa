background(#eef)

def example()
  offset = [3 3]
  angle = time % 1
  loc = [cos(angle) sin(angle)] * 5 + offset
  print(loc)
end

source = example -> branch_ref -> format_source

h = 19

i = 0
for phrase in source
    loc = [10 100 + i * h]
    draw_text(ui_font_medium, phrase[0], #000, loc)

    --print(phrase)
    i += 1
end
