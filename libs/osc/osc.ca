
-- liblo bindings
namespace osc
    def create_server_thread(int port) -> any
    def read_from_server(any server_thread) -> List
    def address(string hostname, int port) -> any
    def send(any address, string path, any values...)
    
    def listen(int port) -> List
        state server_thread = null
        if server_thread == null
            server_thread = create_server_thread(port)

        return read_from_server(server_thread)
