
require Layout

layout = Layout:start([100 100])

layout.setStrategy(:box)

root = layout.cursor

layout.addNode(root, 1)
layout.setProp('height', [:percent 30])

layout.addNode(root, 2)
layout.setProp('height', [:percent 30])

layout.addNode(root, 3)
layout.setProp('height', [:percent 40])

rects = layout.getRects

print(rects)
